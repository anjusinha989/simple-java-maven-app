pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                echo 'Build Successfully'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo 'Tested Successfully'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                echo 'Deployed Successfully'
            }
        }
    }
}
